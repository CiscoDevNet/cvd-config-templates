<config>
  <native xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-native">
    <ntp>
      <server xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-ntp">
        <server-list>
          <ip-address>10.4.48.17</ip-address>
        </server-list>
      </server>
    </ntp>
    <clock>
      <timezone>
          <zone>PST</zone>
          <hours>-8</hours>
          <minutes>0</minutes>
      </timezone>
      <summer-time>
        <zone>PDT</zone>
        <recurring/>
      </summer-time>
    </clock>
    <service>
      <timestamps>
        <debug>
          <datetime>
            <msec>
              <localtime/>
            </msec>
          </datetime>
        </debug>
        <log>
          <datetime>
            <msec/>
            <localtime/>
          </datetime>
        </log>
      </timestamps>
    </service>
  </native>
</config>
